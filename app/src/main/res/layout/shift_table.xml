<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/daily_case"
    android:layout_margin="10dp"
    >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:text="@string/shift"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="@string/line_actual"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="@string/target"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="@string/shift_leak_failure"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:text="@string/start"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:text="@string/end"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlueLine"
                android:textStyle="bold"
                android:background="@drawable/daily_title"
                android:padding="5dp"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_shift1"
                android:gravity="center"
                android:text="@string/add_1st_shift"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_a_s1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_t_s1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_leak_s1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_start_s1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_end_s1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_shift2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:text="@string/add_2nd_shift"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_a_s2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_t_s2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_leak_s2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_start_s2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_end_s2"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_shift3"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:text="@string/add_3rd_shift"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_a_s3"
                android:gravity="center"
                android:textIsSelectable="true"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_t_s3"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tv_leak_s3"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_start_s3"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.2"
                android:id="@+id/tv_end_s3"
                android:gravity="center"
                android:layout_height="match_parent"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:background="@drawable/daily_item"
                android:padding="5dp"
                android:textIsSelectable="true"
                />
        </LinearLayout>

</LinearLayout>